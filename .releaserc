{
  "branches": ["master", "next"],
  "tagFormat": "spring-boot-3.5-java-25-${version}",
  "dryRun": true,
  "debug": true,
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
        "@terrestris/maven-semantic-release",
        {
            "debug": true,
            "mvnw": true
        }
    ],
    "@semantic-release/github"
  ]
}